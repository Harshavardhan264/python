#set operations

x={1,2,3,4,5}
y={4,5,6,7,8}
#union(|)
print(x|y)
#intersection(&)
print(x&y)
#difference(-)
print(x-y)
#set symmetric dufference(^)
print(x^y)
#membership operator
A={'a',1,2,3,5}
print('a' in A)
print(1 not in A)

#output

{1, 2, 3, 4, 5, 6, 7, 8}
{4, 5}
{1, 2, 3}
{1, 2, 3, 6, 7, 8}
True
False
