l=['a','b','c','d','e','f','g']
print(l)
print(l[1])
print(l[1:3])
print(l[1:-1])
print(l[:3])
print(l[2:])
print(l[-4:-1])
print(l[:-1])
print(l[0:5:2])
print(l[1::2])
print(l[::-1])
print(l[::-2])


#output
['a', 'b', 'c', 'd', 'e', 'f', 'g']
b
['b', 'c']
['b', 'c', 'd', 'e', 'f']
['a', 'b', 'c']
['c', 'd', 'e', 'f', 'g']
['d', 'e', 'f']
['a', 'b', 'c', 'd', 'e', 'f']
['a', 'c', 'e']
['b', 'd', 'f']
['g', 'f', 'e', 'd', 'c', 'b', 'a']
['g', 'e', 'c', 'a']
